  "scripts": {
    "start": "DEBUG=app:* node index.js",
    "debug": "rollup -c && DEBUG=app:* node --nolazy --inspect index.js",
    "build": "rollup -c --environment BUILD:production",
    "dev": "DEBUG=app:* DEBUG_COLORS=yes nodemon --watch src --watch ../common/src --exec \"rollup -c && npm start\"",
    "watch": "DEBUG=app:* DEBUG_COLORS=yes jest --runInBand --watch",
    "test": "DEBUG=app:* DEBUG_COLORS=yes jest --runInBand",
    "lint": "eslint \"*.js\" \"src/**/*.js\"",
    "format": "prettier-eslint --write \"$(pwd)/*.js\" \"$(pwd)/src/**/*.js\"",
    "reset": "trash node_modules package-lock.json; npm install"
  },
